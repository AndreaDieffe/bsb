        "soma_radius": 1.5,
        "density": 3e-4
      },
      "morphology": {
        "class": "scaffold.morphologies.NoGeometry"
      },
      "plotting": {
        "display_name": "glomerulus",
        "color": "#0E1030"
      }
    },
    "purkinje_cell": {
      "placement": {
        "class": "scaffold.placement.ParallelArrayPlacement",
        "layer": "purkinje_layer",
        "soma_radius": 7.5,
        "planar_density": 0.003125,
        "extension_x": 130.0,
        "extension_z": 3.5,
        "angle": 4
      },
      "morphology": {
        "class": "scaffold.morphologies.PurkinjeCellGeometry"
      },
      "plotting": {
        "display_name": "purkinje cell",
        "color": "#0F8944"
      }
    },
    "golgi_cell": {
      "placement": {
        "class": "scaffold.placement.LayeredRandomWalk",
        "layer": "granular_layer",
        "soma_radius": 8.0,
        "density": 9e-6
      },
      "morphology": {
        "class": "scaffold.morphologies.GolgiCellGeometry",
        "dendrite_radius": 50.0,
        "axon_x": 150.0,
        "axon_y": 150.0,
        "axon_z": 30.0
      },
      "plotting": {
        "display_name": "golgi cell",
        "color": "#332EBC"
      }
    },
    "stellate_cell": {
      "placement": {
        "class": "scaffold.placement.LayeredRandomWalk",
        "layer": "molecular_layer",
        "soma_radius": 4.0,
        "density": 0.5e-4,
        "y_restriction": [0.66, 1.0]
      },
      "morphology": {
        "class": "scaffold.morphologies.RadialGeometry",
        "dendrite_radius": 15.0
      },
      "plotting": {
        "display_name": "stellate cell",
        "color": "#876506"
      }
    },
    "basket_cell": {
      "placement": {
        "class": "scaffold.placement.LayeredRandomWalk",
        "layer": "molecular_layer",
        "soma_radius": 6.0,
        "density": 0.5e-4,
        "y_restriction": [0.0, 0.66]
      },
      "morphology": {
        "class": "scaffold.morphologies.RadialGeometry",
        "dendrite_radius": 15.0
      },
      "plotting": {
        "display_name": "basket cell",
        "color": "#7A1607"
      }
    },
    "dcn_cell": {
      "placement": {
        "class": "scaffold.placement.LayeredRandomWalk",
        "layer": "dcn_layer",
        "soma_radius": 10.0,
        "placement_relative_to": "purkinje_cell",
        "placement_count_ratio": 0.090909
